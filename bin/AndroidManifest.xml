<?xml version="1.0" encoding="utf-8"?>
<manifest xmlns:android="http://schemas.android.com/apk/res/android"
    package="org.placementcell"
    android:versionCode="1"
    android:versionName="1.0" >

    <uses-sdk
        android:minSdkVersion="19"
        android:targetSdkVersion="19" />

    <application
        android:allowBackup="true"
        android:icon="@drawable/ic_launcher"
        android:label="@string/app_name"
        android:theme="@style/AppTheme" >
        <activity
            android:name="org.placementcell.FlashActivity"
            android:label="@string/app_name" >
            <intent-filter>
                <action android:name="android.intent.action.MAIN" />

                <category android:name="android.intent.category.LAUNCHER" />
            </intent-filter>
        </activity>
        <activity 
            android:name=".LoginActivity"></activity>
        <activity 
            android:name=".RegisterActivity"></activity>
        <activity 
            android:name=".IndexActivity"></activity>
         <activity 
            android:name=".AppActivity"></activity>
     <activity 
            android:name=".CmpActivity"></activity>
     <activity 
            android:name=".CmpRegActivity"></activity>
    
    </application>

</manifest>
